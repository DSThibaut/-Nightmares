<%= link_to house_path(house.id) do %>
  <div class="col-md-4 zoom">
    <% if house.photo.attached? %>
      <%= cl_image_tag house.photo.key, alt: 'Maison des horreurs', class: 'property-image' %>
    <% else %>
      <%= image_tag("citrouille.png") %>
    <% end %>
    <h2><%= house.name %></h2>
    <p>Note: <%= house.rating %>/5</p>
    <p>Prix: <%= house.price %> â‚¬</p>
  </div>
<% end %>
