<h1>Haunted house details</h1>
<p></p>
<h2><%=@house.name%></h2>
<p> Description: <%=@house.description%></p>
<p> Address: <%=@house.address%><p>
<p> Superficy: <%=@house.superficy%><p>
<p> Price: <%=@house.price%><p>
<p> Rating: <%=@house.rating%><p>

<% if current_user.id != @house.user_id %>
  <%= link_to "Louer cette maison!", new_booking_path %>
<% end %>


<%= link_to "Supprimer cette maison", house_path(@house), data: {turbo_method: :delete, turbo_confirm: "Are you sure?"} if policy(@house).destroy? %>
